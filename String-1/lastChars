public String lastChars(String a, String b) {
  if (a.isEmpty() && b.isEmpty()){
    return "@@";
  }
  else if (a.isEmpty()){
    return "@" + b.substring(b.length()-1);
  } else if (b.isEmpty()){
    return a.substring(0,1) + "@";
  }
  else return a.substring(0,1) + b.substring(b.length()-1);
  
}
