public int countYZ(String str) {
  int yzCount = 0;
  for (int i = 0; i<str.length(); i++){
    
    if ((str.charAt(i)=='y' || str.charAt(i)=='z' || str.charAt(i)=='Y' || str.charAt(i)=='Z')) {
    
      if (i==str.length()-1) yzCount++;
      else if (!Character.isLetter(str.charAt(i+1))) yzCount++;
    
  }
}  return yzCount;

}
